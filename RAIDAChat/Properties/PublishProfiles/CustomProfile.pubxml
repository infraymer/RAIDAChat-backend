            <Source Path="DBMigration" DbContext="RAIDAChat.Models.CloudChatEntities, RAIDAChat" MigrationConfiguration="RAIDAChat.Migrations.Configuration, RAIDAChat" Origin="Configuration" />
          </Object>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
    <PrecompileBeforePublish>True</PrecompileBeforePublish>
    <EnableUpdateable>True</EnableUpdateable>
    <DebugSymbols>False</DebugSymbols>
    <WDPMergeOption>DonotMerge</WDPMergeOption>
  </PropertyGroup>
